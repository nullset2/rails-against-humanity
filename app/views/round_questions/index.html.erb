<p id="notice"><%= notice %></p>

<h1>Round Questions</h1>

<table>
  <thead>
    <tr>
      <th>Round</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @round_questions.each do |round_question| %>
      <tr>
        <td><%= round_question.round_id %></td>
        <td><%= round_question.question_id %></td>
        <td><%= link_to 'Show', round_question %></td>
        <td><%= link_to 'Edit', edit_round_question_path(round_question) %></td>
        <td><%= link_to 'Destroy', round_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Round Question', new_round_question_path %>
