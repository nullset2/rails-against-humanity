<p id="notice"><%= notice %></p>

<h1>Round Question Answers</h1>

<table>
  <thead>
    <tr>
      <th>Round question</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @round_question_answers.each do |round_question_answer| %>
      <tr>
        <td><%= round_question_answer.round_question_id %></td>
        <td><%= round_question_answer.user_id %></td>
        <td><%= link_to 'Show', round_question_answer %></td>
        <td><%= link_to 'Edit', edit_round_question_answer_path(round_question_answer) %></td>
        <td><%= link_to 'Destroy', round_question_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Round Question Answer', new_round_question_answer_path %>
